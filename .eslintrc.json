{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": ["react", "react-hooks"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "react/prop-types": "off"
    }
  }
  